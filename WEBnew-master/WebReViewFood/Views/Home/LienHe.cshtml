<!-- ****** Contatc Area Start ****** -->
<div class="contact-area section_padding_80">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="google-map-area">
                    <!--Fix google map geocode-->
                    <div id="googleMap()">
                        <p><iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d15676.481345341474!2d106.7145888!3d10.8020945!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x6c3d29d370b52a7e!2zVHLGsOG7nW5nIMSQ4bqhaSBo4buNYyBIdXRlY2g!5e0!3m2!1svi!2s!4v1592374834841!5m2!1svi!2s" width="600" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe></p>
                    </div>
                </div>
            </div>
        </div>

        <!-- Contact Info Area Start -->
        <div class="contact-info-area section_padding_80_50">
            <div class="row">
                <!-- Single Contact Info -->
                <div class="col-12 col-md-4">
                    <div class="single-contact-info mb-30 text-center wow fadeInUp" data-wow-delay="0.3s">
                        <h4>Hà Nội</h4>
                        <p>10 Láng Hạ, Ba Đình, Hà Nội <br> Email: contact.qntrip@gmail.com <br> Phone: 024.556789</p>
                    </div>
                </div>
                <!-- Single Contact Info -->
                <div class="col-12 col-md-4">
                    <div class="single-contact-info mb-30 text-center wow fadeInUp" data-wow-delay="0.6s">
                        <h4>Quy Nhơn</h4>
                        <p>10 Quang Trung, Ngô Mây, Quy Nhơn <br> Email: contact.qntrip@gmail.com <br> Phone: 026.789655</p>
                    </div>
                </div>
                <!-- Single Contact Info -->
                <div class="col-12 col-md-4">
                    <div class="single-contact-info mb-30 text-center wow fadeInUp" data-wow-delay="0.9s">
                        <h4>TP. Hồ Chí Minh</h4>
                        <p>Lô E2B4, Quận 9, TP. Hồ Chí Minh<br> Email: contact.qntrip@gmail.com <br> Phone: 028.967558</p>
                    </div>
                </div>
            </div>
        </div>

        <!-- Contact Form  -->
        <div class="contact-form-area">
            <div class="row">
                <div class="col-12 col-md-5">
                    <div class="contact-form-sidebar item wow fadeInUpBig" data-wow-delay="0.3s" style="background-image: url(img/bg-img/8.png);">
                    </div>
                </div>
                <div class="col-12 col-md-7 item">
                    <div class="contact-form wow fadeInUpBig" data-wow-delay="0.6s">
                        <h2 class="contact-form-title mb-30">Nếu bạn có bất kỳ câu hỏi nào hãy liên hệ chúng tôi ngay !</h2>
                        <!-- Contact Form -->
                        <form action="#" method="post">
                            <div class="form-group">
                                <input type="text" class="form-control" id="contact-name" placeholder="Tên">
                            </div>
                            <div class="form-group">
                                <input type="email" class="form-control" id="contact-email" placeholder="Email">
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" id="contact-address" placeholder="Địa chỉ">
                            </div>
                            <div class="form-group">
                                <textarea class="form-control" name="message" id="message" cols="30" rows="10" placeholder="Tin nhắn"></textarea>
                            </div>
                            <button type="submit" class="btn contact-btn">Gửi tin nhắn</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

